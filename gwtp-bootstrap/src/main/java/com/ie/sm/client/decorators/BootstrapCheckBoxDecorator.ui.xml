<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
  <ui:style>
    .contents {
        display: inline;
    }
    
    .errorLabel {
        display: none;
        white-space: pre;
    }
  </ui:style>
  <g:HTMLPanel>
    
    <g:SimplePanel ui:field="contents" stylePrimaryName="{style.contents}" />
    <b:HelpInline ui:field="error"></b:HelpInline>
  </g:HTMLPanel>
</ui:UiBinder>




